@model OTMF_NETCORE_MVC.Models.Empleado

@{
    ViewData["Title"] = "Detalles de Empleado";
}
<div class="container">
    <div class="row">
        <div class="offset-3 col-6">
            <h6 class="display-6">
                <strong> Detalles de Empleado</strong>
            </h6>
            <hr />
        </div>

    </div>
<div class="row">
  
    <div class="offset-3 col-6">
     
        <div class="form-group">
           <p>Nombre empleado</p><br /> @Html.DisplayFor(model => model.NombreEmpleado)
        </div>
     
        <div class="form-group">
            @Html.DisplayFor(model => model.ClaveEmpleado)
        </div>

       <div class="form-group">
       <a asp-action="Edit" asp-route-id="@Model?.IdEmpleado" class="btn btn-primary">Edit</a> 
       <a asp-action="Index" class="btn btn-warning">Regresar</a>

       </div>
       
    </div>
</div>


</div>





