@section Scripts{
    <script>
        
$(document).ready(function () {


    $("#Partes").DataTable({
        "ajax": {
            "url": "Partes/ObtenerPartes",
            "type": "GET",
            "datatype": "json"
        },
           dom: 'Bfrtip',
        buttons: [
            {
                extend: 'excelHtml5',
                text: '<i class="fas fa-file-excel"></i>',
                tittleAttr: 'Exportar a Excel',
                className: 'btn btn-lg btn-success'

            },
            {
                extend: 'pdfHtml5',
                text: '<i class="fas fa-file-pdf"></i>',
                tittleAttr: 'Exportar a Excel',
                className: 'btn btn-lg btn-danger'

            }
            , {
                extend: 'print',
                text: '<i class="fa fa-print"></i>',
                tittleAttr: 'Imprimir',
                className: 'btn btn-lg btn-info'

            }
            ,
            {
                text:'<i class="fa fa-plus" aria-hidden="true"></i>',
                tittleAttr: 'Agregar Nuevo',
                className:'btn btn-lg btn-primary',
                action: function() {
                    window.location.href = "Partes/Create";
                }
            }


        ],
        autoFill: false,
        responsive: true,
        select:false,
        rowId:"idParte",
        "columns": [
            { "data": "idParte" },
            { "data": "idCodigoParte" },
            { "data": "aluminio" },
            { "data": "cajasPorTarima" },
            { "data": "costo" },
            { "data": "estandarPorHora" },
            { "data": "piezasPorCaja" },
            { "data": "scrap" },
            { "data": "std_Pintura" },
            { "data": "idCaja" },
            { "data": "nombreCaja" },
            { "data": "idColor" },
            { "data": "nombreColor" },
            { "data": "idPintura" },
            { "data": "nombrePintura" },
            { "data": "idTarima" },
            { "data": "nombreTarima" },
            { "data": "idMolde" },
            { "data": "nombreMolde" },
            { "data": "idEnsamble" },
            { "data": "nombreEnsamble" },
            { "data": "idEstandarConRelevo" },
            { "data": "nombreEstandarConRelevo" },
            { "data": "idEtiqueta" },
            { "data": "nombreEtiqueta" },
            { "data": "idInserto" },
            { "data": "nombreInserto" },
            { "data": "idHule" },
            { "data": "nombreHule" },
            { "data": "idEstandar" },
            { "data": "nombreEstandar" },
            { "data": "idCliente" },
            { "data": "nombreCliente" }
           
        ]

        
    });

   
    var table = $('#Partes').DataTable();
     $('#Partes').on( 'click', 'td', function () {
    var id = table.row( this ).id();
    window.location.href = "Partes/Details/" + id;
     
     });

});


    </script>
}



