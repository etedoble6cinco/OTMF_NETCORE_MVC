@model IEnumerable<OTMF_NETCORE_MVC.Entities.ObtenerRelacionMaquinasUsuarios>
@{
}


<div class="container">

    <div class="row mt-5">
          
         @foreach (var item in Model) {
                <div class="container bg-white">
                  <div class="d-flex justify-content-center">
                      <a class="btn btn-lg btn-primary m-2" asp-controller="OTEstado" asp-action="OrdenesDeTrabajoAsignadas" asp-route-id="@item.IdMaquinaFK">@Html.DisplayFor(modelItem => item.NombreMaquina)</a>
                  </div>
    </div>
  
    
       }

  
 

    </div>
</div>
 
      
  

   


@section Scripts{
     <script src="~/js/pagination.js"></script>
   <script src="~/js/OTEstado/IndexOTEstado.js" type="text/javascript"></script>
  
}